
public class Runner {

	public static void main(String[] args) {
		
		Book b = new Book("b", "bob", "27/03/18");
		System.out.println(b.getItemID());
		Book c = new Book("b","bob", "27/03/18");
		System.out.println(c.getItemID());
		Book d = new Book("c", "cat", "27/03/18");
		System.out.println(d.getItemID());
		
		QALibrary qa = new QALibrary();
		qa.addItem(b);
		qa.addItem(c);
		qa.addItem(d);

	}

}
