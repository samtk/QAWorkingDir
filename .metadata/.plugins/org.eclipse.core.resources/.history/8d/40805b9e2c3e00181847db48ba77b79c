	@Override
	public void checkIn(Item book) {
		// TODO Auto-generated method stub
		itemMap.get(book.getItemID()).changeCheckedOut(true);
		
	}

	@Override
	public void addItem(Item book) {
		itemMap.put(book.getItemID(), book);		
	}

